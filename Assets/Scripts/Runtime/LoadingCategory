using System;

[Flags]
public enum LoadingCategory
{
    None = 0,
    Data = 1 << 0,        // 1
    Visuals = 1 << 1,     // 2
    Analytics = 1 << 2,   // 4
    Network = 1 << 3,     // 8
    GIS = 1 << 4,   // 16
    Styling = 1 << 5,     // 32
    UI = 1 << 6,          // 64
    All = ~0              // Everything
}